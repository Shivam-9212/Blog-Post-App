<%- include('partials/header'); -%>
   <h1>Home</h1>
   <p><%=home_content%></p>
   <%scripts.forEach(function(element){%>
      <%if(element.content.length > 300){%>
        <h1><%=element.head%></h1>
        <p><%=(element.content).substring(0,400)+"..."%><a href="/posts/<%=(element.head).toLowerCase()%>">[Read More]</a></p
      <%}else{%>
        <h1><%=element.head%></h1>
        <p><%=(element.content)%></p>
      <%}%>
   <%})%>
   <form action="compose" method="get">
     <button type="submit" class="btn btn-outline-secondary"><b>+</b> Create</button>
   </form>
  </div>
  <footer>
    <div class="footer-padding">
      <div class="footer">
        <p>Made by Chandan Pathak</p>
      </div>
    </div>
  </footer>
</html>
